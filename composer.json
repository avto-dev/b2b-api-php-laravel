{
    "name": "avto-dev/b2b-api-php-laravel",
    "description": "Laravel 5 package for a working with avto-dev/b2b-api-php",
    "keywords": [
        "laravel",
        "b2b",
        "service"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Avto Develop",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <7.3",
        "avto-dev/b2b-api-php": "^2.1.3",
        "laravel/framework": ">=5.4.3 <5.7.0"
    },
    "require-dev": {
        "laravel/laravel": ">=5.4.3 <5.7.0",
        "mockery/mockery": "0.9.* || ~1.0",
        "phpunit/phpunit": "~5.7.10 || ^6.4 || ~7.0"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\B2BApiLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\B2BApiLaravel\\Tests\\": "tests"
        }
    },
    "support": {
        "issues": "https://github.com/avto-dev/b2b-api-php-laravel/issues",
        "source": "https://github.com/avto-dev/b2b-api-php-laravel"
    },
    "scripts": {
        "test": "@php ./vendor/bin/phpunit --no-coverage",
        "test-cover": "@php ./vendor/bin/phpunit --coverage-html='coverage/html'"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
